{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AppDataFactoryPipeline"
		},
		"AppDataStorageConn_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AppDataStorageConn'"
		},
		"CsvLinkedService_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'CsvLinkedService'"
		},
		"DestinationDataStoreConn_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DestinationDataStoreConn'"
		},
		"AppDataStorageConn_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datafactory203storage.dfs.core.windows.net"
		},
		"CsvLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datafactory203storage.dfs.core.windows.net"
		},
		"DestinationDataStoreConn_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datafactory203storage.dfs.core.windows.net"
		},
		"OnUpload_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/fcdd7d09-54ab-4395-b8ed-f754c640fb8a/resourceGroups/DataFactory203/providers/Microsoft.Storage/storageAccounts/datafactory203storage"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AppDataFactoryPipeline_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AppDataStorageConn_accountKey": {
						"value": "[parameters('AppDataStorageConn_accountKey')]"
					},
					"CsvLinkedService_accountKey": {
						"value": "[parameters('CsvLinkedService_accountKey')]"
					},
					"DestinationDataStoreConn_accountKey": {
						"value": "[parameters('DestinationDataStoreConn_accountKey')]"
					},
					"AppDataStorageConn_properties_typeProperties_url": {
						"value": "[parameters('AppDataStorageConn_properties_typeProperties_url')]"
					},
					"CsvLinkedService_properties_typeProperties_url": {
						"value": "[parameters('CsvLinkedService_properties_typeProperties_url')]"
					},
					"DestinationDataStoreConn_properties_typeProperties_url": {
						"value": "[parameters('DestinationDataStoreConn_properties_typeProperties_url')]"
					},
					"OnUpload_properties_typeProperties_scope": {
						"value": "[parameters('OnUpload_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}